<p id="notice"><%= notice %></p>

<h1>Enfermeros</h1>

<table>
  <thead>
    <tr>
      <th>Ci</th>
      <th>Nro registro</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Direccion</th>
      <th>Ciudade</th>
      <th>Cargo</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Contacto familiar</th>
      <th>Correo</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enfermeros.each do |enfermero| %>
      <tr>
        <td><%= enfermero.ci %></td>
        <td><%= enfermero.nro_registro %></td>
        <td><%= enfermero.nombre %></td>
        <td><%= enfermero.apellido %></td>
        <td><%= enfermero.direccion %></td>
        <td><%= enfermero.ciudade %></td>
        <td><%= enfermero.cargo %></td>
        <td><%= enfermero.telefono %></td>
        <td><%= enfermero.celular %></td>
        <td><%= enfermero.contacto_familiar %></td>
        <td><%= enfermero.correo %></td>
        <td><%= enfermero.foto %></td>
        <td><%= link_to 'Show', enfermero %></td>
        <td><%= link_to 'Edit', edit_enfermero_path(enfermero) %></td>
        <td><%= link_to 'Destroy', enfermero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enfermero', new_enfermero_path %>
