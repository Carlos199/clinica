<p id="notice"><%= notice %></p>

<h1>Medicos</h1>

<table>
  <thead>
    <tr>
      <th>Ci</th>
      <th>Nro registo</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Direccion</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Fecha</th>
      <th>Correo</th>
      <th>Contacto familiar</th>
      <th>Especialidade</th>
      <th>Ciudade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicos.each do |medico| %>
      <tr>
        <td><%= medico.ci %></td>
        <td><%= medico.nro_registo %></td>
        <td><%= medico.nombre %></td>
        <td><%= medico.apellido %></td>
        <td><%= medico.direccion %></td>
        <td><%= medico.telefono %></td>
        <td><%= medico.celular %></td>
        <td><%= medico.fecha %></td>
        <td><%= medico.correo %></td>
        <td><%= medico.contacto_familiar %></td>
        <td><%= medico.especialidade %></td>
        <td><%= medico.ciudade %></td>
        <td><%= link_to 'Show', medico %></td>
        <td><%= link_to 'Edit', edit_medico_path(medico) %></td>
        <td><%= link_to 'Destroy', medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medico', new_medico_path %>
