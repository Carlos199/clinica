<p id="notice"><%= notice %></p>

<h1>Regiones</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regiones.each do |regione| %>
      <tr>
        <td><%= regione.codigo %></td>
        <td><%= regione.nombre %></td>
        <td><%= link_to 'Show', regione %></td>
        <td><%= link_to 'Edit', edit_regione_path(regione) %></td>
        <td><%= link_to 'Destroy', regione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regione', new_regione_path %>
