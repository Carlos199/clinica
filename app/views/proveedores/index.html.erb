<p id="notice"><%= notice %></p>

<h1>Proveedores</h1>

<table>
  <thead>
    <tr>
      <th>Ci</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Ciudade</th>
      <th>Direccion</th>
      <th>Empresa</th>
      <th>Telefono</th>
      <th>Cellular</th>
      <th>Correo</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedores.each do |proveedore| %>
      <tr>
        <td><%= proveedore.ci %></td>
        <td><%= proveedore.nombre %></td>
        <td><%= proveedore.apellido %></td>
        <td><%= proveedore.ciudade %></td>
        <td><%= proveedore.direccion %></td>
        <td><%= proveedore.empresa %></td>
        <td><%= proveedore.telefono %></td>
        <td><%= proveedore.cellular %></td>
        <td><%= proveedore.correo %></td>
        <td><%= proveedore.foto %></td>
        <td><%= link_to 'Show', proveedore %></td>
        <td><%= link_to 'Edit', edit_proveedore_path(proveedore) %></td>
        <td><%= link_to 'Destroy', proveedore, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveedore', new_proveedore_path %>
