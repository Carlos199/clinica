<div class="panel panel-default">
 <div class="panel-heading">Departamentos</div>
 <div class="panel-body">
  <%= render partial: 'layouts/search_form', locals: { search_entity: 'departamentos', word: "Departamentos" } %>
  <br>
  <div class="col-md-5 col-md-offset-5 col-sm-5 col-sm-offset-5">
   <p>
     <%= link_to  new_departamento_path, remote: true, class: "btn btn-primary" do %> 
   <span class="glyphicon glyphicon-plus" aria-hidden="true"> </span> Nuevo Departamento
    <% end %>
    
     <%= link_to  new_ciudade_path, remote: true, class: "btn btn-primary" do %> 
   <span class="glyphicon glyphicon-plus" aria-hidden="true"> </span> Nuevo Ciudades
    <% end %>
   </p>
  </div>
  
  
 </div>
 <%= render partial: 'result_search' %>
</div>